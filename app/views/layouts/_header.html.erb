<div id='header'>
	<div class="container">
		<div class="span-14">
			<div id="header_title" style="font-weight: bold">
				<% if signed_in? %>
					<%=link_to "MuddPage", user_path(current_user)%>
				<% else %>
					<%=link_to "MuddPage", root_url%>
				<% end %>
			</div>
		</div>
		<div class="span-10 last" id="header_menu">
			<%= link_to "Search", search_projects_path %> |  
			<% if signed_in? %>
        <% if admin_user? %>
          <%= link_to "Admin", admin_email_path %> |
        <% end %>
				<%=link_to "My Profile", user_path(current_user) %> | <%= link_to "My Account", edit_user_path(current_user)  %> | <%= link_to "My Feed", user_path(current_user, :page => "feed") %>	| <%= link_to "Log Out", logout_path %>
			<% else %>
				<%= link_to "Sign In", login_path %> | <%= link_to "Sign Up", signup_path %>
			<% end %>
		</div>
	</div>
</div>
