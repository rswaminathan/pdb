<div class="container" style="padding-top:8px; padding-bottom:20px; min-height: 350px">
	<div class="span-14-last", style="margin-right:1em; min-height:20px">
		<% if (1..6).include?(@profile.top_tag_list.size) %>
			<% @profile.top_tag_list.each do |l| %>
				<div class="header_tabs" <%="id = 'current_tab'" if l == params[:tag]%>  >
					<%= link_to l.titleize, user_path(@user, :tag => l)  %>
				</div>
			<% end %>
		<% end %>
	</div>
	<div id="user_background" class="span-24">	
		<table cellspaceing="0" style="margin:0">
			<tbody>
				<tr>
					<td id="profile_div"><%= render 'users/users_soft_sidebar' %></td>
					<td id="user_projects"><%= render 'users/users_sidebar' %></td>
				</tr>
			</tbody>
		</table>
	</div>
</div>