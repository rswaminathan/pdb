<!DOCTYPE html>
<html>
<head>
  <title><%=title %></title>
  <%= csrf_meta_tag %>
  <%= render 'layouts/stylesheets' %>
  <%= render 'shared/analytics' %>
</head>
<body>
   <%= render 'layouts/header' %>


<section class ="round">
	<% flash.each do |key, value| %>
		<div class = "flash <%= key %>"><%= value %></div>
	<% end %>
	
</section>

	<%= yield %>


</body>
<%= render 'layouts/footer' %>
<%= debug(params) if Rails.env.development?%>
</html>
