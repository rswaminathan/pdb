<h1>Edit Contact</h1>
<%= form_for @profile, :url => update_profile_user_path do |f| %>
	<%= render	'shared/error_messages', :object => f.object %>
	<div class="field">
		<%= f.label :website %><br />
		<%= f.text_field :website %>
	</div>
	<div class="field">
		<%= f.label :facebook %><br />
		<%= f.text_field :facebook %>
	</div>
	<div class="field">
		<%= f.label :twitter %><br />
		<%= f.text_field :twitter %>
	</div>
	<div class="field">
		<%= f.label :other %><br />
		<%= f.text_area :other %>
	</div>
	<div class="submit">
		<%= f.submit "Update Contact Page" %><br />
	</div>
<% end %>