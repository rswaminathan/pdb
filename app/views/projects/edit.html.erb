<div class="container" style="padding-top:20px; padding-bottom:20px; min-height: 550px">

<div class="home_register span-12" style="float:center; margin-left: 16em">


<h1>Edit Project</h1>
<%= form_for @project, :html => { :multipart => true } do |f| %>
	<%= render	'shared/error_messages', :object => f.object %>
	<div class="field">
		<%= f.label :name %><br />
		    <div id="project_edit_name"><%= f.text_field :name %></div>
	</div>
	<div class="field">
		<%= f.label :tags %><br />
		<%= f.text_field :kind_list %>
	</div>
	
	<div class="field">
		<%= f.label :Abstract %><br />
		<%= f.text_area :abstract %>
	</div>
	
	<div class="field">
		<%= f.label :description %><br />
		<%= f.text_area :description %>
	</div>
	
	
  <div class="field">
		<%= f.label :project_photo %><br />
		<%= f.file_field :photo %>
	</div>
	<div class="submit">
		<%= f.submit "Edit Project" %><br />
	</div>
	

	
	
<% end %>

</div>

</div>




