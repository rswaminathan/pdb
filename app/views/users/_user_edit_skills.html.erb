<h1>Edit Skills</h1>
<%= form_for @profile, :url => update_profile_user_path do |f| %>
<%= render	'shared/error_messages', :object => f.object %>
	<div class="field">
		<%= f.label :skills %><br />
		<%= f.text_area :skills %>
	</div>
	
	<div class="submit">
		<%= f.submit "Update Skills Page" %><br />
	</div>
<% end %>



